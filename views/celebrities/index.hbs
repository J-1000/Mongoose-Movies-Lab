<h2>Celebrities Listed:</h2>
{{#each celebrityArr}}
    <div>
        <a href="/celebrities/{{this._id}}">{{this.name}}</a>
        <form action="/celebrities/{{this._id}}/delete" method="post">
            <button type="submit">Delete Celebrity</button>
        </form>
        <form action="/celebrities/{{this._id}}/edit" method="get">
            <button type="subtmi">Update Celebrity</button>
        </form>
    </div>
{{/each}}

<a href="/celebrities/new">Add a new celebrity to our list!</a>

