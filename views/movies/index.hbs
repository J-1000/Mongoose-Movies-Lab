<h2>Movies</h2>
<a href="/movies/new">Add a new Movie</a>
{{#each movieList}}
<h3>{{title}}</h3>
<p>{{gengre}}</p>
<p>{{plot}}</p>
{{#each cast}}
<p>Cast: {{this.name}}</p>
{{/each}}

<form action="/movies/{{_id}}/delete" method="POST">
    <button type="submit">Delete this Movie</button>
</form>

<a href="/movies/edit/{{_id}}">
    <button type="submit">Edit this Movie</button>
</a>

{{/each}}