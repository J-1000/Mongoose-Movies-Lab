<h2>Celebrities</h2>
<ul>
    {{#each celeb}}
        <li>
            <a href="/celebrities/{{this._id}}">{{this.name}}</a>
            <form action="/celebrities/{{this._id}}/delete" type="POST">
                <button type="submit">Delete</button>
            </form>
        </li>
    {{/each}}
</ul>
<a href="/celebrities/new">Add a new Celebrity</a>