{{#each Celebrities}}
  <form action="celebrities/{{_id}}/delete" method="post">
    <h2><a href="celebrities/{{_id}}">{{name}}</a></h2>
   <div>{{occupation}}</div> 
    <div>{{catchPhrase}}</div>
    <button type="submit"> Delete </button>
  </form>
  <br>
  <form action="celebrities/{{_id}}/edit" method="get">
    <button type="submit"> Edit</button>
  </form>
{{/each}}